REG DELETE "HKEY_LOCAL_MACHINE\SYSTEM\ControlSet003\Services\cmdHlp" /F
REG DELETE "HKEY_LOCAL_MACHINE\SYSTEM\ControlSet003\Services\Inspect" /F

REG DELETE "HKEY_CLASSES_ROOT\CLSID\{D6267144-4B06-402a-808D-52D498387079}" /F
REG DELETE "HKEY_CLASSES_ROOT\CLSID\{EC1D2C70-8CDE-4013-BE72-2B08A2C54B6B}" /F
